extends layout.pug

block content

  div.header
    p= 'Micro'
    p= 'Proceso'
    p= 'TCC'
    p= 'TE'
    p= 'TVC'
    p= 'TB'
    p= 'TT'
    p= 'TI'
    p= 'TF'
  div.fake
  each task in tasks
    
    if task.wait
      div.card.hueco
        p.item= 'Hueco'
    
    div.card
      p.item= task.micro
      p.item= task.name 
      p.item= task.tcc
      p.item= task.te
      p.item= task.tvc
      p.item= task.tb
      p.item= task.tt - task.ti
      p.item= task.ti
      p.item= task.tt
    

    
   
      
    
   
  
  
    
        
    
  